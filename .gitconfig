[credential "https://github.com"]
	username = ezillinger
[push]
	default = simple
[core]
	editor = vim
[color]
	ui = true
[credential]
	helper = cache --timeout=3600
[alias]
	last = log -1 --stat
	cp = cherry-pick
	co = checkout
	cl = clone
	ci = commit
	st = status -sb
	br = branch
	unstage = reset HEAD --
	dc = diff --cached
	lg = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %Cblue<%an>%Creset' --abbrev-commit --date=relative --all
[rebase]
	autoStash = true
